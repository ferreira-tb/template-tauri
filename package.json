{
  "name": "template-tauri",
  "version": "0.1.28",
  "description": "Tauri template for Manatsu",
  "type": "module",
  "packageManager": "pnpm@8.14.0",
  "private": true,
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json,yaml}": "prettier --write"
  },
  "scripts": {
    "build": "cargo tauri build",
    "dev": "cargo tauri dev",
    "format": "prettier . --write && cargo fmt --manifest-path=src-tauri/Cargo.toml",
    "format-check": "prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "prepare": "husky install",
    "type-check": "vue-tsc --noEmit",
    "vite:dev": "vite",
    "vite:build": "pnpm run type-check && vite build"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.5.3",
    "@vueuse/core": "^10.7.1",
    "manatsu": "^0.2.15",
    "pinia": "^2.1.7",
    "vue": "~3.3.13",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@manatsu/sass": "^0.2.15",
    "@tb-dev/eslint-config": "^1.5.12",
    "@types/node": "^20.10.7",
    "@vitejs/plugin-vue": "^5.0.2",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.1",
    "sass": "^1.69.7",
    "taze": "^0.13.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-vue-devtools": "^7.0.6",
    "vue-tsc": "^1.8.27"
  }
}
