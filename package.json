{
  "name": "tauri-template",
  "version": "0.16.0",
  "description": "Tauri template",
  "type": "module",
  "packageManager": "pnpm@9.6.0",
  "private": true,
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json,yaml}": "prettier --write"
  },
  "scripts": {
    "build": "cargo tauri build --no-bundle",
    "bundle": "cargo tauri build",
    "clippy": "cargo clippy",
    "dev": "cargo tauri dev",
    "dev:devtools": "cargo tauri dev --features open-devtools",
    "format": "prettier . --write && cargo fmt --all",
    "format-check": "prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "lint-fix": "eslint . --config eslint.config.js --fix",
    "prepare": "husky",
    "type-check": "vue-tsc --noEmit",
    "update": "miho update -t",
    "vite:dev": "vite",
    "vite:build": "pnpm run type-check && vite build"
  },
  "dependencies": {
    "@radix-icons/vue": "^1.0.0",
    "@tauri-apps/api": "^2.0.0-beta.15",
    "@tauri-apps/plugin-dialog": "^2.0.0-beta.7",
    "@tauri-apps/plugin-http": "^2.0.0-beta.8",
    "@tauri-apps/plugin-store": "^2.0.0-beta.7",
    "@tauri-apps/plugin-window-state": "^2.0.0-beta.8",
    "@tb-dev/utils": "^2.0.3",
    "@vueuse/core": "^10.11.0",
    "@vueuse/math": "^10.11.0",
    "@vueuse/router": "^10.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "manatsu": "^0.14.0",
    "pinia": "^2.2.0",
    "radix-vue": "^1.9.2",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tauri-plugin-pinia": "^0.2.0",
    "vue": "^3.4.34",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@tb-dev/auto-import-config": "^3.1.0",
    "@tb-dev/eslint-config": "^4.4.2",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.1.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "husky": "^9.1.3",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.7",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.2",
    "vite": "^5.3.5",
    "vite-plugin-vue-devtools": "^7.3.7",
    "vue-tsc": "^2.0.29"
  }
}
