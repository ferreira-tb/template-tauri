{
  "name": "tauri-template",
  "version": "0.13.1",
  "description": "Tauri template",
  "type": "module",
  "packageManager": "pnpm@9.4.0",
  "private": true,
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json,yaml}": "prettier --write"
  },
  "scripts": {
    "build": "cargo tauri build --no-bundle",
    "bundle": "cargo tauri build",
    "clippy": "cargo clippy",
    "dev": "cargo tauri dev",
    "format": "prettier . --write && cargo fmt --all",
    "format-check": "prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "lint-fix": "eslint . --config eslint.config.js --fix",
    "loc": "tokei . -e *.{css,html,js,json,md,toml,yaml} -e **/{autogenerated,entities,ui}",
    "prepare": "husky",
    "type-check": "vue-tsc --noEmit",
    "update": "miho update -t",
    "vite:dev": "vite",
    "vite:build": "pnpm run type-check && vite build"
  },
  "dependencies": {
    "@radix-icons/vue": "^1.0.0",
    "@tauri-apps/api": "^2.0.0-beta.13",
    "@tauri-apps/plugin-dialog": "2.0.0-beta.5",
    "@tauri-apps/plugin-http": "2.0.0-beta.6",
    "@tb-dev/utility-types": "^1.4.2",
    "@tb-dev/utils": "^1.8.5",
    "@vueuse/core": "^10.11.0",
    "@vueuse/math": "^10.11.0",
    "@vueuse/router": "^10.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "manatsu": "^0.12.0",
    "pinia": "^2.1.7",
    "radix-vue": "^1.8.5",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@tb-dev/auto-import-config": "^2.3.0",
    "@tb-dev/eslint-config": "^3.8.6",
    "@types/node": "^20.14.9",
    "@vitejs/plugin-vue": "^5.0.5",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "unplugin-auto-import": "^0.17.6",
    "vite": "^5.3.2",
    "vite-plugin-vue-devtools": "^7.3.5",
    "vue-tsc": "^2.0.24"
  }
}
