{
  "name": "template-tauri",
  "version": "0.7.3",
  "description": "Tauri template for Manatsu",
  "type": "module",
  "packageManager": "pnpm@8.15.4",
  "private": true,
  "lint-staged": {
    "*.{?(c|m)@(j|t)s,css,vue,md,json,yaml}": "prettier --write"
  },
  "scripts": {
    "build": "cargo tauri build",
    "clippy": "cargo clippy -- -D warnings",
    "dev": "cargo tauri dev",
    "format": "prettier . --write && cargo fmt",
    "format-check": "prettier . --check",
    "lint": "eslint . --config eslint.config.js --cache",
    "pedantic": "cargo clippy -- -W clippy::pedantic",
    "prepare": "husky",
    "type-check": "vue-tsc --noEmit",
    "vite:dev": "vite",
    "vite:build": "pnpm run type-check && vite build"
  },
  "dependencies": {
    "@manatsu/style": "^0.6.0",
    "@manatsu/tauri-plugin": "^0.6.0",
    "@tauri-apps/api": "2.0.0-beta.6",
    "@vueuse/core": "^10.9.0",
    "manatsu": "^0.6.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@tb-dev/auto-import-config": "^1.2.8",
    "@tb-dev/eslint-config": "^3.2.2",
    "@tb-dev/utility-types": "^1.2.2",
    "@tb-dev/vue-import-config": "^1.0.4",
    "@types/node": "^20.11.30",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-css-order": "^2.0.1",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.2",
    "vite-plugin-vue-devtools": "^7.0.20",
    "vue-tsc": "^2.0.7"
  }
}
